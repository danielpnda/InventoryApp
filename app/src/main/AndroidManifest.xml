<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inventory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Inventory">
        <activity android:name=".InventoryActivity"></activity>
        <activity android:name=".SupplierActivity" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".CustomerActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".SimpleInventoryActivity" />
        <activity android:name=".ProfitActivity" />
        <activity android:name=".TaxActivity" />
        <activity android:name=".PriceActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ProductsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>