<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".HomePageActivity">

    <ImageButton
        android:id="@+id/productsBtn"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.278"
        app:srcCompat="@drawable/inventory" />

    <ImageButton
        android:id="@+id/priceBtn"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toEndOf="@+id/productsBtn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.278"
        app:srcCompat="@drawable/help" />

    <ImageButton
        android:id="@+id/taxBtn"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/productsBtn"
        app:layout_constraintVertical_bias="0.327"
        app:srcCompat="@drawable/reports" />

    <ImageButton
        android:id="@+id/profitBtn"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toEndOf="@+id/taxBtn"
        app:layout_constraintTop_toBottomOf="@+id/priceBtn"
        app:layout_constraintVertical_bias="0.327"
        app:srcCompat="@drawable/sales" />
</androidx.constraintlayout.widget.ConstraintLayout>